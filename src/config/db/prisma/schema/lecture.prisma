model Lecture {
  id           String        @id() @default(uuid())
  title        String
  createdAt    DateTime      @default(now())
  updatedAt    DateTime      @default(now()) @updatedAt
  ideaContents IdeaContent[]
  position     Int
  videos       Video[]
  topicId      String
  topic        Topic         @relation(fields: [topicId], references: [id])
}
