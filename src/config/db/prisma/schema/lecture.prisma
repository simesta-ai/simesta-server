model Lecture {
  id           String        @id() @default(uuid())
  title        String
  createdAt    DateTime      @default(now())
  updatedAt    DateTime      @default(now()) @updatedAt
  ideaContents IdeaContent[]
  videos       Video[]
  courseId     String
  course       Course        @relation(fields: [courseId], references: [id])
}
